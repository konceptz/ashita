	<inputcommands>
		<cmd input="/dt">
			<gearlock delay="5" />
			<equip set="DT" />
            <command>/ac disable</command>
		</cmd>
        
		<cmd input="/enable">
            <command>/ac enable</command>
		</cmd>
		<cmd input="/changeset ">
			<setvar name="Set" value="%arg1" />
			<addtochat color="159">TP Set: $Set</addtochat>
			<doidlegear />
		</cmd>
		<cmd input="/changeweapon">
			<if eq_main="Chango">
				<equip><main>Ragnarok</main></equip>
			</if>
			<if eq_main="Ragnarok">
				<equip><main>Bravura</main></equip>
			</if>
			<if eq_main="Bravura">
				<equip><main>Chango</main></equip>
			</if>
		</cmd>
	</inputcommands>
	<idlegear>
		<if p_status="engaged">
			<if eq_main="Chango"><equip set="TP-Chan-$Set" /></if>
			<elseif eq_main="ragnarok"><equip set="TP-Rag-$Set" /></elseif>
			<else><equip set="TP-$Set" /></else>
		</if>
		<else><equip set="Stand" /></else>

